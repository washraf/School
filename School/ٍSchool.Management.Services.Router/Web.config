  <system.serviceModel>

    <services>
      <!--Normal Services-->
      <service name="School.Management.Services.Router.NormalService">
        <endpoint name="Normal" address="" contract="School.Management.Services.Router.ICustomerService" binding="wsHttpBinding"/>
      </service>
      <service name="School.Management.Services.Router.PremiumService">
        <endpoint name="Premium" address="" contract="School.Management.Services.Router.ICustomerService" binding="wsHttpBinding"/>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="false"/>
        </behavior>

      </serviceBehaviors>
    </behaviors>
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true" multipleSiteBindingsEnabled="true"/>
